<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Go-Guardian New Validation" parallel="tests" thread-count="4">
    <test name="Chrome Test">
        <classes>
            <class name="com.app.spring.springselenium.newstest.NewsvalidityGoogleChromeTest" />
            <class name="com.app.spring.springselenium.newstest.NewsvalidityYahooChromeTest" />
        </classes>
    </test>
    <test name="Firefox Test">
        <classes>
            <class name="com.app.spring.springselenium.newstest.NewsvalidityGoogleFirefoxTest" />
            <class name="com.app.spring.springselenium.newstest.NewsvalidityYahooFirefoxTest" />
        </classes>
    </test>
</suite>